
<h2><%= fa_icon "list-alt" %>  Timeline</h2>
<% @things_month.each do |month, things| %><hr>
<h3><span class='label label-primary' style="background-color: #555;"><b><%= month.strftime('%B') %> <%= month.strftime('%Y') %></b></span></h3>
<hr>
<% for f in things %>
<% if f.is_a? Medication %>
<div class="panel panel-primary">
  <div class="panel-heading">
    <%= link_to edit_medication_path(f), class: 'btn btn-default btn-sm pull-right', style: 'margin-top: -5px; margin-left: 10px' do %><%= fa_icon 'pencil' %> Edit<% end %>
    <span class="pull-right"><%= f.created_at.strftime("%B %d, %Y at %I:%M %p") %></span>
    <%= fa_icon 'medkit' %> <strong>Medication</strong>
  </div>
  <div class="panel-body">
    <p class="lead">
      Started taking <b><%= f.name %></b>
    </p>
  </div>
</div>


<% elsif f.is_a? DoctorVisit %>
<div class="panel panel-danger">
  <div class="panel-heading">
    <%= link_to edit_doctor_visit_path(f), class: 'btn btn-default btn-sm pull-right', style: 'margin-top: -5px; margin-left: 10px' do %><%= fa_icon 'pencil' %> Edit<% end %>
    <span class="pull-right"><%= f.created_at.strftime("%B %d, %Y at %I:%M %p") %></span>
    <strong><%= fa_icon 'user-md' %> Doctor Visit</strong>
  </div>
  <div class="panel-body">
    <p>
      <%= f.created_at.strftime("%B %d, %Y at %I:%M %p") %> | Visited <b><i><%= if f.has_attribute?(:doctor) then f.doctor end %></i></b> for <b><i><%= f.reason %></i></b> @ <b><i><%= f.location %></i></b>
    </p>
    <p class="lead">
      <%= f.notes %>
    </p>
  </div>
</div>

<% elsif f.is_a? Immunization %>
<div class="panel panel-warning">
  <div class="panel-heading" style="color: black;">
    <%= link_to edit_immunization_path(f), class: 'btn btn-default btn-sm pull-right', style: 'margin-top: -5px; margin-left: 10px' do %><%= fa_icon 'pencil' %> Edit<% end %>
    <span class="pull-right"><%= f.created_at.strftime("%B %d, %Y at %I:%M %p") %></span>
    <strong><%= fa_icon 'bug' %> Immunization</strong>
  </div>
  <div class="panel-body">
    <p class="lead">
      <%= f.name %>
    </p>
  </div>
</div>

<% elsif f.is_a? Symptom %>
<div class="panel panel-primary">
  <div class="panel-heading">
    <%= link_to edit_symptom_path(f), class: 'btn btn-default btn-sm pull-right', style: 'margin-top: -5px; margin-left: 10px' do %><%= fa_icon 'pencil' %> Edit<% end %>
    <span class="pull-right"><%= f.date_occured.strftime("%B %d, %Y at %I:%M %p") %></span>
    <strong><%= fa_icon 'heartbeat' %> Symptom/Event</strong>
  </div>
  <div class="panel-body">
    <p class="lead">
      <%= f.description %>
      <% f.photos.each do |p| %>aaaaaad
      <% image_tag p.image.url %>dsad
      <% end %>
    </p>
  </div>
</div>

<% elsif f.is_a? Condition %>
<div class="panel panel-info">
  <div class="panel-heading" style="color: black;">
    <%= link_to edit_condition_path(f), class: 'btn btn-default btn-sm pull-right', style: 'margin-top: -5px; margin-left: 10px' do %><%= fa_icon 'pencil' %> Edit<% end %>
    <span class="pull-right"><%= f.created_at.strftime("%B %d, %Y at %I:%M %p") %></span>
    <strong><%= fa_icon 'stethoscope' %> Diagnosis/Condition</strong>
  </div>
  <div class="panel-body">
    <p class="lead">
      <%= f.name %>
    </p>
  </div>
</div>

<% elsif f.is_a? Allergy %>
<div class="panel panel-success">
  <div class="panel-heading">
    <%= link_to edit_allergy_path(f), class: 'btn btn-default btn-sm pull-right', style: 'margin-top: -5px; margin-left: 10px' do %><%= fa_icon 'pencil' %> Edit<% end %>
    <span class="pull-right"><%= f.created_at.strftime("%B %d, %Y at %I:%M %p") %></span>
    <%= fa_icon 'leaf' %> <strong>Allergy</strong>
  </div>
  <div class="panel-body">
    <p class="lead">
      <b>Allergy: </b><%= f.name %>
      <br>
      <b>Notes: </b><%= f.description %>
    </p>
  </div>
</div>

<% end %>
<% end %>
<% end %>